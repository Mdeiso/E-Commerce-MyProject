<h1>Your Catalog</h1>
<% for product in @products -%>
<div class = "entry">
	<%= image_tag(product.image_url) %>
	<h3><%=h product.title %></h3>
	<%= product.description %>
	<span class = "price"><%= product.price %></span>
	<!--<%= button_to "Add to Cart", :controller => :cart, :action => :index %>-->
</div>
<% end %>